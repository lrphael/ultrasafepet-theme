    "f1aab89f-0866-49a9-a9bc-6379a720fa45": {
      "type": "image-with-text",
      "blocks": {
        "template--16893937877309__f1aab89f-0866-49a9-a9bc-6379a720fa45-166844814516d7d79b-0": {
          "type": "heading",
          "settings": {
            "heading": "This is your opportunity!",
